{
  "description": "字帖生成器微信小程序项目配置",
  "packOptions": {
    "ignore": [
      {
        "type": "folder",
        "value": ".claude"
      },
      {
        "type": "folder", 
        "value": "prototype"
      },
      {
        "type": "folder",
        "value": "dist"
      },
      {
        "type": "file",
        "value": "*.ts"
      },
      {
        "type": "file",
        "value": "*.md"
      }
    ]
  },
  "setting": {
    "urlCheck": false,
    "es6": true,
    "enhance": true,
    "postcss": true,
    "minified": false,
    "newFeature": true,
    "coverView": true,
    "nodeModules": true,
    "autoAudits": false,
    "showShadowRootInWxmlPanel": true,
    "checkInvalidKey": true,
    "checkSiteMap": true,
    "uploadWithSourceMap": true,
    "compileHotReLoad": false,
    "useMultiFrameRuntime": true,
    "useApiHook": true,
    "babelSetting": {
      "ignore": [],
      "disablePlugins": [],
      "outputPath": ""
    },
    "bundle": false,
    "useIsolateContext": true,
    "useCompilerModule": true,
    "userConfirmedUseCompilerModuleSwitch": false,
    "packNpmManually": false,
    "packNpmRelationList": [
      {
        "packageJsonPath": "./package.json",
        "miniprogramNpmDistDir": "./"
      }
    ]
  },
  "compileType": "miniprogram",
  "libVersion": "2.32.0",
  "appid": "wx-your-appid-here",
  "projectname": "zitie_mini",
  "debugOptions": {
    "hidedInDevtools": []
  },
  "scripts": {},
  "simulatorType": "wechat",
  "simulatorPluginLibVersion": {},
  "condition": {
    "search": {
      "current": -1,
      "list": []
    },
    "conversation": {
      "current": -1,
      "list": []
    },
    "game": {
      "current": -1,
      "list": []
    },
    "miniprogram": {
      "current": -1,
      "list": []
    }
  }
}
