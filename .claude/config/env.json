{
  "development": {
    "debug": true,
    "log_level": "debug",
    "cache_ttl_seconds": 300,
    "max_retries": 3,
    "timeout_ms": 30000
  },
  "production": {
    "debug": false,
    "log_level": "info", 
    "cache_ttl_seconds": 3600,
    "max_retries": 5,
    "timeout_ms": 60000
  },
  "testing": {
    "debug": true,
    "log_level": "debug",
    "cache_ttl_seconds": 60,
    "max_retries": 1,
    "timeout_ms": 10000
  },
  "default_environment": "development",
  "github": {
    "api_base_url": "https://api.github.com",
    "rate_limit_buffer": 100
  },
  "cache": {
    "max_size_mb": 100,
    "cleanup_interval_seconds": 3600,
    "max_entries": 1000
  },
  "security": {
    "allowed_commands": ["git", "npm", "yarn", "node", "python", "pip"],
    "max_command_length": 1000,
    "sandbox_mode": false
  }
}